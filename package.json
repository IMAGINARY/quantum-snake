{
  "name": "quantum-snake",
  "version": "0.0.0",
  "description": "Classic snake game with a quantum twist",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IMAGINARY/quantum-snake.git"
  },
  "author": {
    "name": "Christian Stussak",
    "email": "christian.stussak@imaginary.org"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IMAGINARY/quantum-snake/issues"
  },
  "homepage": "https://imaginary.github.io/quantum-snake/index.html",
  "source": [
    "src/html/index.html"
  ],
  "engines": {
    "npm": ">=10.1.0",
    "node": ">=20.9.0"
  },
  "browserslist": [
    ">= 0.5%",
    "last 2 major versions",
    "not dead",
    "not Explorer <= 11",
    "not KaiOS > 0",
    "not OperaMini all"
  ],
  "devDependencies": {
    "@parcel/macros": "^2.12.0",
    "@parcel/packager-ts": "^2.12.0",
    "@parcel/plugin": "^2.12.0",
    "@parcel/transformer-inline-string": "^2.12.0",
    "@parcel/transformer-sass": "^2.12.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-compat": "^4.2.0",
    "eslint-plugin-import": "^2.29.1",
    "npm-check-updates": "^16.14.15",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.12.0",
    "parcel-namer-rewrite": "^2.10.3-rc.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "stylelint": "^16.2.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "tsx": "^4.7.1"
  },
  "@parcel/resolver-default": {
    "packageExports": true
  },
  "@parcel/bundler-default": {
    "manualSharedBundles": [
      {
        "name": "vendor",
        "root": "src/js/main.js",
        "assets": [
          "node_modules/**/*"
        ]
      }
    ]
  },
  "parcel-namer-rewrite": {
    "silent": true,
    "rules": {
      "(.*).html": "$1.html",
      "(.*)\\.([^.]*)": "assets/$1.{hash}.$2"
    }
  },
  "scripts": {
    "serve": "parcel serve --dist-dir dist-dev",
    "clean": "rimraf assets src/html/index.html",
    "prebuild": "npm run clean",
    "build": "parcel build --no-cache --public-url '.' --dist-dir '.'",
    "format": "prettier --write '.'",
    "lint": "npm-run-all -s -c lint:**",
    "lint:scripts": "eslint src",
    "lint:styles": "stylelint 'src/{**/*,*}.{css,scss}'",
    "fix": "npm-run-all -s -c fix:**",
    "fix:scripts": "eslint --fix src",
    "fix:styles": "stylelint --fix 'src/{**/*,*}.{css,scss}'",
    "check-updates": "npm-check-updates"
  },
  "dependencies": {
    "mathjs": "^14.0.1",
    "quantum-circuit": "^0.9.227"
  }
}
